{
    "name": "wyrihaximus/react-http-middleware-resume-response-body",
    "description": "react/http middleware that resumes response bodies if they are streaming",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "psr/http-message": "^1",
        "react/event-loop": "^1.1",
        "react/http": "^1",
        "react/promise": "^2.7",
        "react/stream": "^1.1"
    },
    "require-dev": {
        "ringcentral/psr7": "^1.2.2",
        "wyrihaximus/async-test-utilities": "^3.3.1"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\Http\\Middleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\React\\Tests\\Http\\Middleware\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
